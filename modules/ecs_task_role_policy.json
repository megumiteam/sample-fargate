{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "logs:CreateLogStream"
            ],
            "Resource": [
                "arn:aws:logs:${region}:${aws_id}:log-group:${awslogs}-log:*"
            ],
            "Effect": "Allow"
        },
        {
            "Action": [
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:${region}:${aws_id}:log-group:${awslogs}-log:*"
            ],
            "Effect": "Allow"
        }
    ]
}